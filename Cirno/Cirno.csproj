<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>cirno_icon_one..ico</ApplicationIcon>
	<Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="main.crn" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="cirno_icon_one..ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="main.crn">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

</Project>
